{
  "name": "multi-ai-translator",
  "version": "1.0.0",
  "description": "Browser extension for translating web pages using multiple AI providers",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "package": "node scripts/package.js"
  },
  "keywords": [
    "chrome-extension",
    "edge-extension",
    "firefox-addon",
    "translation",
    "ai",
    "llm",
    "openai",
    "claude",
    "gemini",
    "ollama"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.2.1",
    "@types/chrome": "^0.0.268",
    "@types/webextension-polyfill": "^0.12.4",
    "archiver": "^7.0.1",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "vite": "^7.2.2",
    "vite-plugin-static-copy": "^3.1.4",
    "webextension-polyfill": "^0.12.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@google/generative-ai": "^0.17.1",
    "ollama": "^0.5.8",
    "openai": "^4.57.0"
